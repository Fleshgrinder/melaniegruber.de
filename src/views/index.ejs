<% include partials/head %>
<h1 class="sr-only"><%= page.title %></h1>
<div class="gallery">
    <div class="intro">
        <div class="intro-content"><%- contents %></div>
    </div>
    <% page.projects.forEach(function (project) { %>
        <a class="img-anchor project rel" href="<%= project.route %>">
            <article class="project-wrapper">
                <div class="abs project-overlay">
                    <h2 class="project-title rel"><%= project.title %></h2>
                    <% if (project.programs.length > 0 ) { %>
                        <p class="abs-before project-subtitle rel"><%- page.renderProgramIcons(project.programs) %></p>
                    <% } %>
                </div>
                <picture><%- page.renderIndexTile(project) %></picture>
            </article>
        </a>
    <% }); %>
</div>
<% include partials/foot %>
