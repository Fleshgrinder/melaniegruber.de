<% if (programIcon.isVector) { %>
    <img alt="<%= programIcon.title %> icon" class="program-icon" height="<%= programIcon.height %>" src="<%= programIcon.src(page.index) %>" title="<%= programIcon.title %>" width="<%= programIcon.width %>">
<% } else { %>
    <picture title="<%= programIcon.title %>">
        <source srcset="<%= programIcon.srcSet(page.index, 'webp') %>" type="image/webp">
        <img alt="" class="program-icon" height="<%= programIcon.height %>" src="<%= programIcon.src(page.index) %>" srcset="<%= programIcon.srcSet(page.index) %>" width="<%= programIcon.width %>">
    </picture>
<% } %>
